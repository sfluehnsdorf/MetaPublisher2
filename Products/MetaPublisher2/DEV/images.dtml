<dtml-let
  page_title="'DEV'"
  page_info="'DEV DEV DEV'"
  management_view="'Images'"
  >
<dtml-var manage_MetaPublisher2_header>



  <h3 class="hpad">Images</h3>

  <table class="list" cellspacing="0" cellpadding="4" border="0">

    <tr class="list-header">
      <th width="1%"><div class="form-label">#</div></th>
      <th width="1%"><div class="form-label">Image</div></th>
      <th><div class="form-label">Identifier</div></th>
    </tr>

<dtml-in DEV_list_product_images prefix=image>
    <tr class="row-<dtml-if image_odd>normal<dtml-else>hilite</dtml-if>">
      <td valign="top"><div class="list-item"><dtml-var image_number></div></td>
      <td valign="top"><div class="list-item">
<dtml-if "image_item.startswith( 'ATTR ' )">
        <a target="_blank" href="<dtml-var "image_item[ 5: ]">"><img src="<dtml-var "image_item[ 5: ]">"></a>
</dtml-if>
<dtml-if "image_item.startswith( 'PROD ' )">
        <a target="_blank" href="/misc_/MetaPublisher2/<dtml-var "image_item[ 5: ]">"><img src="/misc_/MetaPublisher2/<dtml-var "image_item[ 5: ]">"></a>
</dtml-if>
      </div></td>
      <td valign="top"><div class="list-item"><dtml-var image_item></div></td>
    </tr>
</dtml-in>

  </table>


<dtml-var manage_MetaPublisher2_footer>
</dtml-let>
